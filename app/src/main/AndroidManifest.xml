<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.yifeihappy.planechess">

    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.example.aty.AtyMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.aty.AtyCreateRoom"/>
        <activity android:name="com.example.aty.AtySearchRoom"/>
        <activity android:name="com.example.aty.AtyInstruction"/>
        <activity android:name="com.example.aty.AtyWaitClients"/>
        <activity android:name="com.example.aty.AtyClientSetting"></activity>
        <activity android:name="com.example.aty.AtyGameClient"></activity>
        <activity android:name="com.example.aty.AtyGameSever"></activity>
    </application>

</manifest>
